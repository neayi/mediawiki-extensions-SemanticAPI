{
    "name": "SemanticAPI",
    "version": "1.0.0",
    "author": "Bertrand Gorge",
    "url": "https://www.mediawiki.org/wiki/Extension:SemanticAPI",
    "descriptionmsg": "semanticapi-desc",
    "type": "extension",
    "requires": {
        "MediaWiki": ">= 1.43"
    },
    "load_composer_autoloader": true,
    "AutoloadNamespaces": {
        "MediaWiki\\Extension\\SemanticAPI\\": "includes/"
    },
    "manifest_version": 2,
    "RestRoutes": [
        {
            "path": "/semanticproperty/{title}",
            "method": "GET",
            "class": "MediaWiki\\Extension\\SemanticAPI\\RestGetSemanticPropertyHandler"
        },
        {
            "path": "/semanticproperty/{title}", 
            "method": "POST",
            "class": "MediaWiki\\Extension\\SemanticAPI\\RestSetSemanticPropertyHandler"
        },
        {
            "path": "/semanticproperty/{title}/{property}", 
            "method": "DELETE",
            "class": "MediaWiki\\Extension\\SemanticAPI\\RestDeleteSemanticPropertyHandler"
        }
    ],
    "MessagesDirs": {
        "SemanticAPI": [
            "i18n"
        ]
    }
}